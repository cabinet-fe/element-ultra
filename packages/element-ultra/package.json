{
  "name": "element-ultra",
  "version": "0.3.18",
  "description": "A Component Library for Vue 3",
  "type": "module",
  "keywords": [
    "element-ultra",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "scripts": {
    "dts": "pnpm vue-tsc --declaration --emitDeclarationOnly"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.mjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./*.mjs": "./*.mjs",
    "./*": "./*"
  },
  "main": "index.mjs",
  "module": "index.mjs",
  "style": "dist/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/element-ultra.git"
  },
  "bugs": {
    "url": "https://github.com/cabinet-fe/element-ultra/issues"
  },
  "peerDependencies": {
    "vue": "^3.2.47",
    "vue-router": ">=4.1.6"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "@popperjs/core": "^2.11.2",
    "@vueuse/core": "^8.1.2",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "lodash-unified": "^1.0.1",
    "memoize-one": "^6.0.0",
    "sortablejs": "^1.15.0",
    "normalize-wheel-es": "^1.1.2",
    "@wangeditor/editor": "^5.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178"
  }
}
